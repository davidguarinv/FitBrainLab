{
  "name": "fitbrain-lab",
  "version": "1.0.0",
  "description": "FitBrain Lab - Cognitive Fitness Platform",
  "scripts": {
    "build:css": "tailwindcss -i ./static/css/input.css -o ./static/css/output.css --minify --config ./static/css/config/tailwind.config.js --postcss ./static/css/config/postcss.config.js",
    "watch:css": "tailwindcss -i ./static/css/input.css -o ./static/css/output.css --watch --config ./static/css/config/tailwind.config.js --postcss ./static/css/config/postcss.config.js",
    "build": "npm run build:css",
    "dev": "npm run watch:css",
    "tailwind": "npm run watch:css",
    "tailwind:build": "npm run build:css"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "tailwindcss-animate": "^1.0.7"
  }
}
